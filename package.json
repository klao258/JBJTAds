{
  "name": "jbjtads",
  "version": "1.0.0",
  "description": "JBJTAds project with koa2 and mongoose",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node app.js",
    "client": "vite --config client/vite.config.js --host --port 5173",
    "build": "vite build --config client/vite.config.js",
    "start": "npm run build && node app.js"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "mongoose": "^7.5.0",
    "naive-ui": "^2.42.0",
    "telegraf": "^4.16.3",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^9.2.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^5.0.0"
  }
}
